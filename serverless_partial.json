{
  "service": "github-assignment-uploader",
  "provider": {
    "name": "aws",
    "runtime": "nodejs10.x",
    "stage": "prod",
    "region": "us-east-2",
    "environment": {
      "variable1": "value1"
    },
    "exclude": [
      "test/**",
      ".git/**",
      ".env.example",
      ".env",
      "LICENSE",
      "README.md",
      "travis.yml",
      "app.yml"
    ]
  },
  "functions": {
    "probot": {
      "handler": "handler.probot",
      "timeout": 30,
      "events": [
        {
          "http": {
            "path": "/",
            "method": "any"
          }
        },
        {
          "http": {
            "path": "/{any+}",
            "method": "any"
          }
        }
      ]
    }
  }
}